{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/app.js",
    "client": "cd ../client && npm start",
    "server": "nodemon src/app.ts",
    "dev": "concurrently --kill-others-on-fail \"npm run client\" \"npm run server\"",
    "build": "tsc && npm run copy-files",
    "create-db": "ts-node ./src/migration/create-db.ts",
    "create-table-bau": "ts-node ./src/migration/create-table-bau.ts",
    "create-table-baut": "ts-node src/migration/create-table-baut.ts",
    "create-table-blu": "ts-node ./src/migration/create-table-blu.ts",
    "create-table-blut": "ts-node ./src/migration/create-table-blut.ts"
  },
  "dependencies": {
    "@aws-sdk/abort-controller": "^3.110.0",
    "@aws-sdk/client-s3": "^3.121.0",
    "@types/multer": "^1.4.7",
    "aws-sdk": "^2.1171.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "concurrently": "^7.2.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.18",
    "sequelize": "^6.21.2",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.3",
    "@types/sequelize": "^4.28.14",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
